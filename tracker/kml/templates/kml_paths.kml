<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Document>
<name>People</name>
	<Style id="sn_ylw-pushpin">
		<IconStyle>
			<color>ff0000ff</color>
			<scale>0.5</scale>
			<Icon>
				<href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
			</Icon>
			<hotSpot x="20" y="20" xunits="pixels" yunits="pixels"/>
		</IconStyle>
		<LabelStyle>
      <color>7fffaaff</color>
      <scale>1.5</scale>
    </LabelStyle>
    <LineStyle>
      <color>ff0000ff</color>
      <width>1</width>
    </LineStyle>
    <PolyStyle>
      <color>7f7faaaa</color>
      <colorMode>random</colorMode>
    </PolyStyle>
    </Style>
	<StyleMap id="msn_ylw-pushpin">
		<Pair>
			<key>normal</key>
			<styleUrl>#sn_ylw-pushpin</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#sh_ylw-pushpin</styleUrl>
		</Pair>
	</StyleMap>
	<Style id="sh_ylw-pushpin">
		<IconStyle>
			<color>ff0000ff</color>
			<scale>1.3</scale>
			<Icon>
				<href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
			</Icon>
			<hotSpot x="20" y="20" xunits="pixels" yunits="pixels"/>
		</IconStyle>
		<LabelStyle>
			<color>ff0000aa</color>
		</LabelStyle>
		    <LineStyle>
      <color>ff0000ff</color>
      <width>1</width>
    </LineStyle>
    <PolyStyle>
      <color>7f7faaaa</color>
      <colorMode>random</colorMode>
    </PolyStyle>
	</Style>
{%for user in users%}{%if user.hasPath%}
<Placemark>
    <name>{{user}} Start</name>
    <description>{{user}}</description>
    <styleUrl>#sh_ylw-pushpin</styleUrl>
    <Point>
      <coordinates>{{user.path.0.longitude}},{{user.path.0.latitude}},0</coordinates>
    </Point>
</Placemark>
<Placemark>
    <name>{{user}}'s Path</name>
    <description></description>
    <styleUrl>#sn_ylw-pushpin</styleUrl>
          <LineString>
        <altitudeMode>relative</altitudeMode>
        <coordinates>{%for point in user.path%}
        {{point.longitude}},{{point.latitude}},0{%endfor%}
        </coordinates></LineString>
        
</Placemark>
<Placemark>
    <name>{{user}} End</name>
    <description>{{user}}</description>
    <styleUrl>#sh_ylw-pushpin</styleUrl>
    <Point>
      <coordinates>{{user.lastPoint.longitude}},{{user.lastPoint.latitude}},0</coordinates>
    </Point>
</Placemark>
{%endif%}{%endfor%}
</Document>
</kml>